<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VizualizareAvocatActivity">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        app:srcCompat="@color/grenaInchis" />

    <Button
        android:id="@+id/button_trimite_solicitare"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="108dp"
        android:background="@color/gray"
        android:text="Trimite solicitare colaborare"
        android:textAlignment="center" />



    <TextView
        android:id="@+id/textView_nume_avocat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="30dp" />


    <TextView
        android:id="@+id/textView_specializare_avocat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="198dp"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView_precizare_specializare_avocat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:layout_centerVertical="true"
        android:text="TextView" />
</RelativeLayout>